package sec2;

import java.net.ServerSocket;
import java.net.Socket;

public class MultiServer {

	public void start() {
		ServerSocket server = null;
		Socket socket = null;
		server = new ServerSocket(8000);
		
	}
	
	public static void main(String[] args) {
		MultiServer multi = new MultiServer();
		multi.start();
	
	
	}

}
