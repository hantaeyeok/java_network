package sec2;

//Thread(쓰레드) = process(프로세스) = 현재 컴퓨터에서 실행 중인 프로그램(작업)
//하나의 프로그램 하나의 Thread
public class ThreadExam1 {
	
	public static void FirstThread() {
		System.out.println("첫 번째 Thread");
	}

	public static void SeconaryThread() {
		System.out.println("두 번째 Thread");
	}
	
	public static void main(String[] args) {

//		FisrtThread();	:	이렇케 호출했지만
		Thread thread1 = new Thread(new Runnable());	//실행가능한 작업
//		Runnable : 인터페이스 이기때문에 오버라이딩을 받아야함
		
		Thread thread2 = new Thread();
		
	}

}
